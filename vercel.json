{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/app.py"
        }
    ],
    "headers": [
        {
            "source": "/static/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}